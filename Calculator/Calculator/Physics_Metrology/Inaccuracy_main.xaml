<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Calculator.Inaccuracy_main">

    <ContentPage.Resources>
        <ResourceDictionary>
            <x:Array x:Key="Inaccuracy" Type="{x:Type x:String}">
                <x:String>Расчет погрешности по классу точности прибора</x:String>
            </x:Array>
        </ResourceDictionary>
    </ContentPage.Resources>
    
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Выберите способ нахождения погрешности" FontSize="Large"/>
            <ListView ItemsSource="{StaticResource Key= Inaccuracy }" ItemTapped="InaccuracyList_ItemTapped"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>